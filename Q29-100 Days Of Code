//Q29: Write a program to calculate the factorial of a number.

/*
Sample Test Cases:
Input 1:
5
Output 1:
120

Input 2:
3
Output 2:
6

*/
#include <stdio.h>

int main()
{
   int n;
   int factorial = 1;
   scanf("%d", &n);

   if(n==0){
    printf("%d\n",0);
   }else{
    for (int i = n; i >= 1; i--) {
        factorial *= i;
    }
    printf("%d\n",factorial);
   }


    return 0;
}
